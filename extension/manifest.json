{
    "manifest_version": 3,
    "name": "SWSS",
    "version": "1.1",
    "description": "Decodes HTML files encrypted with RSA",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }]
}